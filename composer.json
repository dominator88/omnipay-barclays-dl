{
    "version": "3.0.0",
    "name": "dominator88/omnipay-barclays-dl",
    "type": "library",
    "description": "Barclays ePDQ Direct Link driver for the Omnipay payment processing library.",
    "keywords": [
        "gateway",
        "merchant",
        "omnipay",
        "pay",
        "payment",
        "barclays",
        "epdq",
        "directlink"
    ],
    "homepage": "https://github.com/dominator88/omnipay-barclays-dl",
    "license": "MIT",
    "authors": [{
        "name": "MR.Z",
        "email": "zsh2088@gmail.com"
    }],
    "autoload": {
        "psr-0": {
            "Omnipay\\BarclaysEpdqDl\\": "src/"
        }
    },
    "require": {
        "php-http/guzzle6-adapter": "^2.0.1",
        "omnipay/common": "~3.0"
    },
    "require-dev": {
        "omnipay/tests": "~3.0",
        "squizlabs/php_codesniffer": "^3"
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 src/",
        "fix-style": "phpcbf -p --standard=PSR2 src/"
    },
    "prefer-stable": true
}